#: import Diagram modeful.ui.diagram.Diagram
#: import ModeBar modeful.ui.modebar.ModeBar
#: import Window kivy.core.window.Window


<Editor>:
    tabarea: tabarea
    orientation: 'vertical'
    ModeBar:
        size_hint_y: None
        height: '30dp'
    TabbedPanel:
        id: tabarea
        size: Window.size
        do_default_tab: False
